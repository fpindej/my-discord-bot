{
  "DiscordBotConfiguration": {
    "Token": "<DISCORD_BOT_TOKEN>",
    "LogLevel": "Info"
  },
  "OpenAiConfiguration": {
    "ApiKey": "<OPENAI_API_KEY>",
    "TextAi": {
      "TextModelType": "GPT4_Turbo",
      "DefaultChatContext": "<DEFAULT_CHAT_CONTEXT>",
      "CacheSlidingExpiration": "00:00:30"
    },
    "ImageAiConfiguration": {
      "ImageModelType": "DALLE3"
    },
    "AudioAiConfiguration": {
      "AudioModelType": "TTS_HD"
    }
  },
  "CoinApiConfiguration": {
    "BaseUrl": "https://rest.coinapi.io",
    "ApiKey": "<API_KEY>",
    "HandlerLifetime": "5:00:00",
    "RetryCount": 3
  },
  "MongoDbConfiguration": {
    "ConnectionString": "<CONNECTION_STRING>",
    "DatabaseName": "<DATABASE_NAME>"
  },
  "Serilog": {
    "Using":  [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" }
    ]
  }
}