{
  "DiscordBotConfiguration": {
    "Token": "<DISCORD_BOT_TOKEN>",
    "LogLevel": "Info"
  },
  "OpenAiConfiguration": {
    "ApiKey": "<OPENAI_API_KEY>",
    "Model": "GPT4_Turbo",
    "CacheSlidingExpiration" : "0:30:00"
  },
  "CoinApiConfiguration": {
    "BaseUrl": "https://rest.coinapi.io",
    "ApiKey": "<API_KEY>",
    "HandlerLifetime": "5:00:00",
    "RetryCount": 3
  },
  "Serilog": {
    "Using":  [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" }
    ]
  }
}